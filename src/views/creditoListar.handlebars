<div class="wrapper">
  <!-- Main content -->
  <section class="content">
    <div class="container-fluid">
      <div class="row">
        <!-- Left col -->
        <div class="col-md-12">
          <!-- TABLE: LATEST ORDERS -->
          <div class="card">
            <div class="card-header border-transparent">
              <h3 class="card-title">Informaci√≥n de pedidos</h3>
              <br><br>
              <label for="formGroupExampleInput2">Buscar</label>
              <br>
              <div class="container-fluid row">
                <div class="container col-10">
                  <input type="number" class="form-control" placeholder="Buscar por Id" name="Activo" id="Activo"
                    required>
                </div>
                <div class="container col-2">
                  <a href="/app/credito/create/" class="btn btn-info float-right">Buscar</a>
                </div>
              </div>
              <br>
              <div class="card-tools">
                <button type="button" class="btn btn-tool" data-card-widget="collapse">
                  <i class="fas fa-minus"></i>
                </button>
                <button type="button" class="btn btn-tool" data-card-widget="remove">
                  <i class="fas fa-times"></i>
                </button>
              </div>
              <div class="card-body p-0">
                <div class="table-responsive">
                  <table class="table m-0">
                    <thead>
                      <tr>
                        <th>ID</th>
                        <th>ID Venta</th>
                        <th>Activo</th>
                      </tr>
                    </thead>
                    <tbody>
                      {{#each lista}}
                      <tr>
                        <td>
                          {{this.dataValues.IdCredito}}
                        </td>
                        <td>
                          {{this.dataValues.IdVenta}}
                        </td>
                        <td>

                </div>
                <!-- /.card-header -->{{this.dataValues.Activo}}
                </td>
                </tr>
                {{/each}}
                </tbody>
                </table>
              </div>
              <!-- /.table-responsive -->
            </div>
            <!-- /.card-body -->
            <div class="card-footer clearfix">
              <a href="/app/credito/create/" class="btn btn-sm btn-primary float-left">Nuevo</a>
            </div>
            <!-- /.card-footer -->
          </div>
          <!-- /.card -->
        </div>
        <!-- Fin columna izquierda -->
        <!-- Fin columna derecha -->
      </div>
      <!-- Fin primera fila -->
    </div>
  </section>